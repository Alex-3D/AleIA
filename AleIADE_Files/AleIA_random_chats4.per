(defrule
	(timer-triggered chatTimer)
	(current-age >= castle-age)
	(goal ATTACK YES)
	(not (goal oceanmap YES))
	(not (goal EnabledAttack NO))
	(population < 200)
=>
	(chat-to-allies "¡¡¡¡¡¡MUERTE... MUERTE A TODO AQUEL QUE NOS ESTORBEEEEEE!!!!!!")
	(chat-to-allies "/31")
	(disable-timer chatTimer)
	(enable-timer chatTimer 300)
)

(defrule
	(timer-triggered chatTimer)
	(current-age >= castle-age)
	(goal ATTACK YES)
	(not (goal oceanmap YES))
	(not (goal EnabledAttack NO))
	(population >= 200)
=>
	(chat-to-allies "¡¡¡¡HAAAAAAAAAAAAAAAAAAA LA CARGAAAAAAAAAAAAAA!!!!!")
	(chat-to-allies "/31")
