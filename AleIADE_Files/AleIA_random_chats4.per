
(defrule
  (game-time >= 20)
=>
(chat-to-all "40")
(chat-to-all "Otra vez vos...?")
(disable-self)
)

(defrule
	(current-age == dark-age)
	(players-current-age any-enemy == feudal-age)
=>
	(chat-to-all "Segui corriendo nomas...")
	(disable-self)
)

